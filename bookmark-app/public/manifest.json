{
  "name": "Bookmark App",
  "short_name": "Bookmarks",
  "description": "Save, organize, and sync bookmarks across all devices",
  "start_url": "/",
  "display": "standalone",
  "background_color": "#0f0f0f",
  "theme_color": "#0f0f0f",
  "icons": [
    {
      "src": "/icons/icon-192.png",
      "sizes": "192x192",
