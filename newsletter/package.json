{
  "name": "newsletter",
  "type": "module",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "astro dev",
    "build": "astro build && npx pagefind --site dist",
    "preview": "astro preview",
    "astro": "astro",
    "d1:create": "wrangler d1 create newsletter-db",
    "d1:migrate": "wrangler d1 execute newsletter-db --file=db/schema.sql",
    "d1:migrate:local": "wrangler d1 execute newsletter-db --file=db/schema.sql --local"
  },
  "dependencies": {
    "@astrojs/cloudflare": "^12.0.0",
    "@astrojs/rss": "^4.0.0",
    "astro": "^5.5.0",
    "resend": "^4.0.0"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20250214.0",
    "pagefind": "^1.3.0",
    "typescript": "^5.7.0",
    "wrangler": "^4.0.0"
  }
}
